<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>

	<typeAliases>
		<typeAlias type="com.green.biz.product.dto.ProductVO" alias="product"></typeAlias>
		<typeAlias type="com.green.biz.member.MemberVO" alias="member"></typeAlias>
		<typeAlias type="com.green.biz.address.AddressVO" alias="address"></typeAlias>
		<typeAlias type="com.green.biz.order.CartVO" alias="cart"></typeAlias>
		<typeAlias type="com.green.biz.order.OrderVO" alias="order"></typeAlias>
		<typeAlias type="com.green.biz.qna.QnaVO" alias="qna"></typeAlias>
		<typeAlias type="com.green.biz.admin.EmployeeVO" alias="worker"></typeAlias>
		<typeAlias type="com.green.biz.product.dto.Paging" alias="paging"></typeAlias>
	</typeAliases>
	
	<mappers>
		<mapper resource="mappings/product-mapping.xml" />
		<mapper resource="mappings/member-mapping.xml" />
		<mapper resource="mappings/cart-mapping.xml" />
		<mapper resource="mappings/order-mapping.xml" />
		<mapper resource="mappings/qna-mapping.xml" />
		<mapper resource="mappings/admin-mapping.xml" />
	</mappers>
	
</configuration>
